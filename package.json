{
  "name": "fanup",
  "version": "1.0.0",
  "description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"200\" alt=\"Nest Logo\" /></a> </p>",
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit --silent",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage --forceExit  && coveralls < ./coverage/lcov.info"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.3",
    "babel-jest": "^28.0.0",
    "coveralls": "^3.1.1",
    "jest": "28.1.3",
    "jest-environment-jsdom": "^29.3.1",
    "ts-jest": "^28.0.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": [
      ".*\\.spec\\.ts$",
      ".*\\.e2e-spec\\.ts$",
      ".*\\.test\\.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "babel.config.js",
      "App.tsx",
      "coverage",
      "dist",
      ".d.ts",
      ".css",
      "config-overrides.js",
      "reportWebVitals.ts",
      "setupTests.ts",
      "Routes.ts",
      "client/src/hooks",
      "client/src/images",
      "client/src/mocks",
      "client/src/services",
      "client/src/style",
      "client/src/types",
      "client/src/components/icons"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/client/node_modules/",
      "<rootDir>/server/gateway/node_modules/",
      "<rootDir>/server/services/auth/node_modules/",
      "<rootDir>/server/services/core/node_modules/",
      "<rootDir>/server/services/ticket/node_modules/"
    ]
  }
}
