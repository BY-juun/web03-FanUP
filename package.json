{
  "name": "fanup",
  "version": "1.0.0",
  "description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo-small.svg\" width=\"200\" alt=\"Nest Logo\" /></a> </p>",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "jest": "jest --coverage",
    "coveralls": "jest --coverage && coveralls < ./coverage/lcov.info"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "coveralls": "^3.1.1",
    "jest": "28.1.3",
    "ts-jest": "28.0.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": [
      ".spec.ts$",
      ".e2e-spec.ts$"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "client",
      "coverage",
      "server/gateway/dist",
      "server/services/api/dist",
      "server/services/auth/dist",
      "server/services/ticket/dist"
    ]
  }
}
