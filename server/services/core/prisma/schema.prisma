generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Chat {
    id Int @id @default(autoincrement())
    fanup_id String
    email String
    is_artist Boolean
    message String
    created_at DateTime @default(now())
}

model FileEntity {
  id Int @id @default(autoincrement())
  link String @unique
  name String?
  mimeType String?
}