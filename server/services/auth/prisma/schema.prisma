// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  FAN
  ARTIST
}
model User {
  id          Int     @default(autoincrement()) @id
  role        Role    @default(FAN)
  providerId  String
  provider    String
  email       String?
  nickname    String?
}